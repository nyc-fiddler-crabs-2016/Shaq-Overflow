<div id = "new-user-form">
<%= form_for @user do |f|%>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <div id = "new-user-button"><%= f.submit %></div>
<% end %>
</div>
